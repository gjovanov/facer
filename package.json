{
  "name": "facer",
  "version": "1.0.0",
  "description": "Facer - Realtime Face Recognition in the browser",
  "author": "Goran Jovanov",
  "private": true,
  "scripts": {
    "api": "cross-env NODE_ENV=development node api/start.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production pm2 start server/index.js -i 0 --attach -f",
    "start-simple": "cross-env NODE_ENV=production node ./server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/vuetify": "^1.9.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "face-api.js": "^0.21.0",
    "multer": "^1.4.2",
    "nuxt": "^2.10.2",
    "pm2": "^4.2.1",
    "rimraf": "^3.0.0",
    "sharp": "^0.23.3",
    "vuetify": "^2.1.11",
    "webrtc-adapter": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "browser-env": "^3.2.6",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": ">=10.0.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^2.0.0",
    "standard": "^14.3.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  }
}
